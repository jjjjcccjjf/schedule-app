// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:dev.db"
}

model schedule {
  id Int @id @default(autoincrement())
  from_date DateTime
  to_date DateTime
  room_number String
  subject_name String
}